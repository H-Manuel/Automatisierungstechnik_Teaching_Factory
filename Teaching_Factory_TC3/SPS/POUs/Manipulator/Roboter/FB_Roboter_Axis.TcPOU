  <POU Name="FB_Roboter_Axis" Id="{bb71503e-a414-410e-8892-e81fc9490a92}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_Roboter_Axis
VAR_INPUT
	bEnable :BOOL;
	bExecute :BOOL;
	rPosition :REAL;
	bRefSens :BOOL;
END_VAR
VAR_OUTPUT
	eState :eBetriebszustand;
END_VAR
VAR
	nState:INT :=0;
	mc_Reset :MC_Reset;
	mc_MoveAbsolute :MC_MoveAbsolute;
	mc_Home :MC_Home;
	mc_Power :MC_Power;
	mc_HomingMode1: MC_HomingMode;
	
END_VAR
VAR_IN_OUT
	ax :AXIS_REF;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[ax.ReadStatus();


mc_Home(
	Axis:= ax, 
	Execute:= , 
	Position:= 0, 
	HomingMode:= mc_HomingMode1, 
	BufferMode:= , 
	Options:= , 
	bCalibrationCam:= bRefSens, 
	Done=> , 
	Busy=> , 
	Active=> , 
	CommandAborted=> , 
	Error=> , 
	ErrorID=> );
	
mc_MoveAbsolute(
	Axis:= ax, 
	Execute:= , 
	Position:= , 
	Velocity:= 30, 
	Acceleration:= 30, 
	Deceleration:= 30, 
	Jerk:= , 
	BufferMode:= , 
	Options:= , 
	Done=> , 
	Busy=> , 
	Active=> , 
	CommandAborted=> , 
	Error=> , 
	ErrorID=> );

mc_Power(
	Axis:= ax, 
	Enable:= , 
	Enable_Positive:= , 
	Enable_Negative:= , 
	Override:= , 
	BufferMode:= , 
	Options:= , 
	Status=> , 
	Busy=> , 
	Error=> , 
	ErrorID=> );
	
mc_Reset(
	Axis:= ax, 
	Execute:= , 
	Done=> , 
	Busy=> , 
	Error=> , 
	ErrorID=> );
	
//IF NOT mc_Power.Status THEN
//	nState:=0;
//END_IF

IF NOT bEnable THEN
	nState :=0;
END_IF

CASE nState OF 
	0:
	eState:=eBetriebszustand.Off;
	mc_Power.Enable :=TRUE;
	mc_Power.Enable_Positive := TRUE;  
	mc_Power.Enable_Negative := TRUE;

	
	
	IF bEnable AND NOT mc_power.Status THEN
		nState :=10;
	ELSIF mc_power.Status AND bEnable THEN
		nState:=12;
	END_IF
	
	10: // init Reset
	eState:=eBetriebszustand.Init;
	mc_Reset.Execute:=TRUE;
	//mc_Power.Enable:=FALSE;
	
	IF mc_Reset.Done THEN
		nState:=11;
		mc_Reset.Execute:=FALSE;
	END_IF
	
	11: // init Power
	eState:=eBetriebszustand.Init;
	
	mc_Power.Enable :=TRUE;
	//mc_Power.Enable_Positive:=TRUE;
	
	
	IF mc_Power.Status THEN
		nState:=12;
	END_IF
	
	12://Homed?
	eState:=eBetriebszustand.Init;
	
	IF ax.Status.Homed THEN
		nstate:=20;
	ELSE
		nstate:=13;
	END_IF
	
	13://Homing
	eState:=eBetriebszustand.Init;
	
	mc_Home.Execute :=TRUE;
	
	IF mc_Home.Done THEN
		mc_Home.Execute :=FALSE;
		nstate:= 20;
	END_IF
	
	
	20: // Ready
	eState:= eBetriebszustand.Ready;
	
	IF bExecute THEN 
		nState:=30;
	END_IF
 
	30: // Bewegung
	eState:=eBetriebszustand.Production;
	mc_MoveAbsolute.Position := rPosition;
	mc_MoveAbsolute.Execute :=TRUE;
	
	IF mc_MoveAbsolute.Done THEN 
		mc_MoveAbsolute.Execute:=FALSE;
		nState :=40;
	END_IF
	
	40: // Fertig
	eState :=eBetriebszustand.Done;
	
	IF NOT bExecute THEN 
		nState:=20;
	END_IF

END_CASE	
]]></ST>
    </Implementation>
